{
  "name": "@kerj/react-parallax-path",
  "version": "0.1.0",
  "description": "Scroll-driven parallax animations with SVG path following for React",
  "author": "kerj",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "keywords": [
    "react",
    "parallax",
    "scroll",
    "animation",
    "svg",
    "path",
    "motion",
    "path-following",
    "scroll-driven"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kerj/react-parallax-path"
  },
  "bugs": {
    "url": "https://github.com/kerj/react-parallax-path/issues"
  },
  "homepage": "https://github.com/kerj/react-parallax-path#readme",
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "typescript": "^5.1.6",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.9.0"
  },
  "scripts": {
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  }
}
